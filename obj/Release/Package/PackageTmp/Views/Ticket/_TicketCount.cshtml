@model IEnumerable<BugTracker.Models.Ticket>

<!-- Visitor Info Box -->
<div class="col-sm-4 col-xs-6">
    <!-- Box info -->
  
        <div class="box-info">
            <!-- Icon box -->
            <div class="icon-box">
                <span class="fa-stack">
                    <i class="fa fa-circle fa-stack-2x warning"></i>
                    <i class="fa fa-ticket fa-stack-1x fa-inverse"></i>
                </span>
            </div><!-- End div .icon-box -->
            <!-- Text box -->
            <div class="text-box">
                @if (Model != null)
                {

                    <h3>@Model.Count()</h3>
                }
                else

                {
                    <h3>0</h3>
                }
                <p>Tickets Assigned</p>
            </div><!-- End div .text-box -->
            <div class="clear"></div>

            <a href="@Url.Action("Create","Ticket")"> <p class="text-center">Click to create a Ticket </p>   </a>
</div><!-- End div .info-box -->
 
</div>
<!-- End Visitor Info Box -->